		<instruction text="&#34;Eingabe: U&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
		<instruction text="&#34;Ausgabe:Geben Sie die Stromstärke (I) ein:&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
		<instruction text="&#34;Eingabe: I&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
		<alternative text="&#34;Rechnung = &#34;&#34;Widerstand&#34;&#34; oder &#34;&#34;R&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" disabled="0">
			<qTrue>
				<instruction text="&#34;berechnung R = U/I&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
				<instruction text="&#34;Ausgabe: &#34;&#34;Der Widerstand ist: &#34;&#34; + R&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
			</qTrue>
